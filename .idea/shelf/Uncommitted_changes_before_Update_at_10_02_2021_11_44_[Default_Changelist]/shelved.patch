-TIMEOUTCODE timeout(const char *cmd)
-{
-  t_dotcmd c;
-  c = execdotcmd(cmd,".wait",NULL);
-  switch(c) {
-    case ENTER_CMD:
-         return CODE_ENTER;
-    case ESCAPE_CMD:
-         return CODE_ESCAPE;
-    default:
-         return CODE_WAIT;
-  }
-}
